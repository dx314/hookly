    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^5.48.2",
    "svelte-check": "^4.3.5",
    "svelte-eslint-parser": "^1.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.3.1"
